<!DOCTYPE html>
<html>
    <head>
        <!-- required meta tags -->
        <meta charset="utf-8">
        <meta name="description" content="Welcome to the official website of PRISE.">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <!-- title -->
        <title>Node App</title>
        <!-- CSS -->
        <link rel="stylesheet" href="/css/style.css">
    </head>
    <body>
        <h1>PRISE</h1>
        <table>
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>
            <% items.forEach((item) => { %>
                <tr>
                    <th><%= item.id %></th>
                    <th><%= item.name %></th>
                </tr>
            <% }) %>
        </table>
        <a href="/new">Add a new user</a>
    </body>
</html>
